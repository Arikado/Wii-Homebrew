extern void do_reset();

void timercallback(syswd_t alarm, void *cb)
{
 count++;
 if(count > 2)
 {
  printf("%d\n",count);
  GX_AbortFrame();
 }

 if(count > 10)
 {
  kill = 1;
 }

 FPS = incFPS;
 incFPS = 0;
}

